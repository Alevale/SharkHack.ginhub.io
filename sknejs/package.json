{
  "name": "sknejs",
  "version": "2.0.0",
  "description": "Opskins bot manager",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "readme": "node ./node_modules/.bin/node-readme",
    "cleanup": "node dist/worker/cleanup.js",
    "worker": "node dist/worker/sknexchange.js",
    "worker:dev": "babel-watch src/worker/sknexchange.js",
    "build": "./node_modules/.bin/babel -d ./dist ./src",
    "start": "babel-watch src/index.js",
    "prices": "babel-watch src/prices.js",
    "prices:debug": "babel-node debug src/prices.js",
    "sales": "babel-node src/worker/opskins.js",
    "sales:debug": "babel-node debug src/worker/opskins.js",
    "start:admin": "babel-watch src/admin/index.js",
    "acl:role": "babel-node src/scripts/aclRoles.js",
    "acl:resource": "babel-node src/scripts/aclResource.js",
    "apikeys": "babel-node src/scripts/apiKeys.js",
    "generateUser": "babel-node src/scripts/generateUser.js",
    "generateCertificate": "babel-node src/scripts/generateCertificate.js",
    "createBot": "babel-node src/scripts/createBot.js"
  },
  "main": "dist/index.js",
  "keywords": [
    "node",
    "babel",
    "test",
    "tape",
    "boilerplate"
  ],
  "files": [
    "dist/*",
    "README.md",
    "LICENCE"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
